{
  "name": "github-cli",
  "version": "1.0.0",
  "description": "Command-line interface for GitHub",
  "repository": "https://github.com/tiagoaguiar100/github-cli",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node --env-file=.env.local dist/index.js",
    "build": "tsc --project tsconfig.json && copyfiles -u 1 **/sql/*.sql dist/sql",
    "deploy": "npm run build && npm install -g .",
    "lint": "npx eslint --fix **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tiago Aguiar",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^5.4.0",
    "chalk": "^4.1.2",
    "copyfiles": "^2.4.1",
    "pg-promise": "^11.9.1",
    "ramda": "^0.30.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.5.4",
    "@types/ramda": "^0.30.2",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript-eslint": "^8.4.0"
  },
  "bin": {
    "github-cli": "./dist/index.js"
  }
}
